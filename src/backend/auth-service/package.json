    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "speakeasy": "^2.0.0",
    "mongoose": "^8.0.3",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "rate-limit-redis": "^4.0.0",
